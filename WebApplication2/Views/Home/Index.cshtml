                        "infoEmpty": "尚無資料",
                        "infoFiltered": "(從 _MAX_ 筆資料過濾)",
                        "infoPostFix": "",
                        "search": "搜尋字串:",
                        "paginate": {
                            "first": "首頁",
                            "last": "末頁",
                            "next": "下頁",
                            "previous": "前頁"
                        },
                        "aria": {
                            "sortAscending": ": 升冪",
                            "sortDescending": ": 降冪"
                        }
                    },
                    //[顯示比數],[項目選擇]
                    lengthMenu: [[50], [50]],

                    //asc升序、desc降序
                    order:[[0,"asc"]],

                    //取消項目排序功能
                    ordering: true,

                    //分頁功能
                    paging: false,

                    //搜尋欄位是否顯示
                    searching: true,

                    //指定高跟寬
                    //scrollX: "500px",
                    //scrollY: "400px",

                });

            });
